{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery.min.js", "js/jquery.observe.min.js", "js/jquery.caret.min.js", "js/dislike.js" ],
	  "css" : ["style/default.css"],
      "matches": [ "http://*/*", "https://*/*" ],
	  "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Dislike button for Facebook",
   "homepage_url": "http://idislike.hatemzidi.com/",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   
   "manifest_version": 2,
   "name": "Dislike button for Facebook",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "contextMenus", "webNavigation", "notifications", "cookies", "webRequest" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.6.3",
   "web_accessible_resources": [ "js/*", "images/*", "style/*", "fonts/*" ]
}
